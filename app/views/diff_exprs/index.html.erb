<p id="notice"><%= notice %></p>

<h1>Diff Exprs</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @diff_exprs.each do |diff_expr| %>
      <tr>
        <td><%= link_to 'Show', diff_expr %></td>
        <td><%= link_to 'Edit', edit_diff_expr_path(diff_expr) %></td>
        <td><%= link_to 'Destroy', diff_expr, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Diff Expr', new_diff_expr_path %>
