<p id="notice"><%= notice %></p>

<h1>Filter Methods</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @filter_methods.each do |filter_method| %>
      <tr>
        <td><%= link_to 'Show', filter_method %></td>
        <td><%= link_to 'Edit', edit_filter_method_path(filter_method) %></td>
        <td><%= link_to 'Destroy', filter_method, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Filter Method', new_filter_method_path %>
